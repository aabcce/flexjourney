<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>

	<table name="jy_attend" catalog="journey">
		<primary-key>
			<generator class="native"></generator>
		</primary-key>
	</table>
	<table name="jy_party" catalog="journey">
		<primary-key>
			<generator class="native"></generator>
		</primary-key>
	</table>
	<table name="jy_user" catalog="journey">
		<primary-key>
			<generator class="native"></generator>
		</primary-key>
	</table>
	<table name="jy_photo" catalog="journey">
		<primary-key>
			<generator class="native"></generator>
		</primary-key>
	</table>
	<table name="jy_article" catalog="journey">
		<primary-key>
			<generator class="native"></generator>
		</primary-key>
		<column name="describe"></column>
		<column name="content"></column>
	</table>
</hibernate-reverse-engineering>